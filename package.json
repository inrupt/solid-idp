  "scripts": {
    "clean": "rimraf coverage tmp",
    "build": "tsc -p tsconfig.json && npm run copy-template",
    "watch": "tsc --watch --preserveWatchOutput -p tsconfig.json",
    "dev": "ts-node-dev --project \"tsconfig.json\" examples/koa.ts",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "mocha": "ts-mocha -p \"tsconfig.json\" test/**/*.ts",
    "coverage": "nyc ts-mocha -p \"tsconfig.json\" test/**/*.ts",
    "test": "npm run lint && npm run mocha",
    "copy-template": "copyfiles ./src/**/*.ejs ./dist && copyfiles ./src/**/*.css ./dist"
  }
